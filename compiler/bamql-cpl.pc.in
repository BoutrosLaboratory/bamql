prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
bindir=@bindir@
includedir=@includedir@
datarootdir=@datarootdir@
datadir=@datadir@

Name: BAMQL Compiler
Description: Filter SAM/BAM reads using a query language
Version: @VERSION@
Requires:
Requires.private: htslib libpcre
Libs: -L${libdir} -lbamql-cpl
Libs.private: -L${libdir} -lbamql-cpl -L@LLVM_CORE_LDFLAGS@ @LLVM_CORE_LIBS@
Cflags: -I${includedir}
Cflags.private: -I${includedir} @LLVM_CORE_CPPFLAGS@
